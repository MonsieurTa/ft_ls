directory					:	blue 	/	default
symlink						:	magenta	/	default
socket						:	green	/	default
pipe						:	brown	/	default
executable					:	red		/	default
block spe					:	blue	/	cyan
char spe					:	blue	/	brown
exec w/ setuid				:	black	/	red
exec w/ setgid				:	black	/	cyan
dir w/ write + sticky bit	:	black	/	green
dir w/ write w/o sticky bit	:	black	/	brown
